    <!--era-template-state-->
    <!--era-template-styles-->
</head>

<body>
    <div id="app"></div>
    <% if(node_env == 'prod') { %>
      <!--线上版本-->
      <script crossorigin="anonymous" src="//www.dpfile.com/app/owl/static/owl_1.6.3.js"></script>
    <% } else { %>
      <!--线下版本-->
      <script crossorigin="anonymous" src="//s1.51ping.com/app/owl/static/owl_1.6.3.js"></script>
    <% } %>
    <script>
      var devMode = true;
      var project = 'zc-fe-merchant';
      var env = '<%=node_env%>';

      if (env == 'prod') {
        devMode = false;
        project = 'zc-fe-merchant';
      }

      Owl && Owl.start({
        devMode: devMode,
        project: 'zc-fe-merchant',
        pageUrl: 'index'
      })
    </script>
    <!--era-template-scripts-->
</body>
</html>
